# Supabase Postgres
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?pgbouncer=true&connection_limit=1"
DIRECT_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Supabase API (Settings âžœ API)
# Project URL, anon key for client-side Auth helpers, and service role key for server-side calls
NEXT_PUBLIC_SUPABASE_URL="https://YOUR_PROJECT_REF.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
SUPABASE_SERVICE_ROLE_KEY="YOUR_SUPABASE_SERVICE_ROLE_KEY"

# Cloudflare R2 (S3 API)
R2_ACCOUNT_ID="xxxxxxxxxxxxxxxxxxxxx"
R2_ACCESS_KEY_ID="XXXXXXXXXXXXXXXX"
R2_SECRET_ACCESS_KEY="YYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYY"
R2_BUCKET="plant-care"

# One of these for public image URLs
NEXT_PUBLIC_R2_PUBLIC_HOSTNAME="pub-xxxxxxxxxxxxx.r2.dev"
NEXT_PUBLIC_R2_CUSTOM_HOSTNAME=""

# Optional species enrich (leave blank to skip)
TREFLE_TOKEN=""

# Optional: default lat/lon for weather snapshots
DEFAULT_LAT="44.9778"
DEFAULT_LON="-93.2650"
